<!DOCTYPE html>
<html lang="en">
  <head>
    <title>{{ title }}</title>
    <link rel="stylesheet" href="{{ cssSrc }}">
    {% include "head.njk" %}
    <!-- ** Scripts -->
    {% include "scripts.njk" %}
  </head>
  <body>
    {# Fixed reference #}
    <div id="fixedReference"></div>
    {# Cursor #}
    <div class="cursor">
      <div class="cursor-ball">
        <svg height="14" width="14">
          <circle cx="7" cy="7" r="6" stroke-width="0"></circle>
        </svg>
      </div>
      <div class="cursor-outer">
        <svg height="50" width="50">
          <circle class="cursor-outer-circle" cx="25" cy="25" r="24"></circle>
        </svg>
      </div>
    </div>
    {# Page change #}
    <div class="pageChangeContainer --contain">
        <p id="pageChangeText">{{ name }}</p>
    </div>
    {% include "partials/navigation.njk" %}
    <main>{% block content %} {{ content | safe }} {% endblock %}</main>
    {# {% include "partials/footer.njk" %} #}
  </body>
  <script content-type="text/javascript" src="{{ jsSrcBase }}"></script>
  <script content-type="text/javascript" src="{{ jsSrc }}"></script>
</html> 